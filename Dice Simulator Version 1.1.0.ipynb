    "import random\n",
    "while True:\n",
    "    Dice=random.randint(1,6)\n",
    "    RollDice=input(\"To roll Dice Press R \")\n",
    "    if RollDice.lower()=='r':\n",
    "        print(Dice)\n",
    "    elif RollDice.lower()=='q':\n",
    "        break\n",
    "    else:\n",
    "        print(\"Press R to roll & Q to Quit\")"
   ]
